    "autoload": {
        "psr-4": {
            "Jayflashy\\LicenseChecker\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Joshua Adeyemi",
            "email": "jadesdevelopers@gmail.com"
        }
